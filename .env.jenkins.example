# Jenkins CI/CD Environment Variables
# Copy this file to .env.jenkins and fill in your values

# ngrok Configuration
NGROK_AUTH_TOKEN=your_ngrok_auth_token_here

# Database Configuration
POSTGRES_PASSWORD=your_secure_postgres_password
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=your_secure_rabbitmq_password

# API Keys and Secrets
GROQ_API_KEY=your_groq_api_key
AUTH0_DOMAIN=your_auth0_domain
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret

# Deployment Tokens (optional)
RAILWAY_TOKEN=your_railway_token
VERCEL_TOKEN=your_vercel_token

# Notification Webhooks (optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# Docker Registry (optional)
DOCKER_REGISTRY=your_registry_url
DOCKER_USERNAME=your_docker_username
DOCKER_PASSWORD=your_docker_password

# Test Configuration
TEST_DATABASE_URL=postgresql://testuser:testpass@localhost:5433/test_db
TEST_RABBITMQ_URL=amqp://admin:admin@localhost:5673/

# Staging Configuration
STAGING_API_URL=http://localhost:8000
STAGING_FRONTEND_URL=http://localhost:3000